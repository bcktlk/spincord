{
  "name": "spincord",
  "version": "1.0.0",
  "description": "a discord bot for getting album info in chat",
  "main": "dist/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kmrn/spincord.git"
  },
  "author": "Kamran Payne",
  "license": "MIT",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "build": "tsc",
    "postbuild": "cp package.json dist/package.json && cd dist && yarn install --production",
    "dev": "ts-node-dev src/app.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "register": "tsc && node dist/utils/deploy_commands"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.1.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.27.2",
    "discord.js": "^13.6.0"
  }
}
